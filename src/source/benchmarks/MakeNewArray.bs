@suite()
namespace MakeNewArray
    @test("empty = []")
    sub _()
        empty = []
    end sub

    @test("empty = createObject (resize true)")
    sub _()
        empty = createObject("roArray", 0, true)
    end sub

    @test("empty = createObject (resize false)")
    sub _()
        empty = createObject("roArray", 0, false)
    end sub
end namespace
